{
   "manifest_version":2,
   "name":"Chrome Extimsion",
   "version":"0.1",
   "web_accessible_resources":[
      "img/*.jpg",
      "img/*.jpeg"
   ],
   "permissions":[
      "tabs",
      "storage",
      "notifications",
      "http://*/",
      "https://*/"
   ],
   "content_scripts":[
      {
         "matches":[
            "<all_urls>"
         ],
         "js":[
            "tim.js"
         ]
      }
   ],
   "background":{
      "browser_action":{
         "default_icon":"img/tim.png"
      },
      "scripts":[
         "background.js"
      ]
   },
   "icons":{
      "48":"img/tim.png"
   }
}
